# ## {{ ansible_managed }}

[user]
default={{ wsl_user | default(user_name) }}

[network]
generateResolvConf = true
generateHosts = true
hostname = "{{ wsl_hostname | default('wsl') }}"

[interop]
enabled=true
appendWindowsPath=true

[automount]
enabled=true
options="metadata"

[boot]
systemd=true
